{
  "datasets": [
    {
      "name": "a39a255d",
      "displayName": "Relatorio - Top 20 Market Cap Médio)",
      "queryLines": [
        "SELECT\r\n",
        "  id_moeda, simbolo, nome_moeda,\r\n",
        "  valor_mercado_ultimo_dia_bi_usd,\r\n",
        "  volume_ultimo_dia_bi_usd,\r\n",
        "  qtd_dias_com_dados,\r\n",
        "  data_ultimo_dia\r\n",
        "FROM coingecko.gold.gold_resumo_moeda\r\n",
        "ORDER BY valor_mercado_ultimo_dia_usd DESC\r\n",
        "LIMIT 20;"
      ]
    },
    {
      "name": "10976382",
      "displayName": "Preço Atual das Moedas (Última Data Disponível)",
      "queryLines": [
        "WITH ultima_data AS (\r\n",
        "  SELECT max(data_referencia) AS data_mais_recente\r\n",
        "  FROM coingecko.silver.silver_fato_moeda_diaria\r\n",
        ")\r\n",
        "SELECT\r\n",
        "  s.data_referencia AS data_mais_recente,\r\n",
        "  s.id_moeda,\r\n",
        "  s.simbolo,\r\n",
        "  s.nome_moeda,\r\n",
        "  s.preco_medio_usd AS preco_usd\r\n",
        "FROM coingecko.silver.silver_fato_moeda_diaria s\r\n",
        "JOIN ultima_data u\r\n",
        "  ON s.data_referencia = u.data_mais_recente\r\n",
        "ORDER BY s.preco_medio_usd DESC;\r\n"
      ]
    },
    {
      "name": "a4101159",
      "displayName": "Pizza – Market Cap",
      "queryLines": [
        "WITH base AS (\r\n",
        "  SELECT\r\n",
        "    nome_moeda,\r\n",
        "    simbolo,\r\n",
        "    valor_mercado_ultimo_dia_usd\r\n",
        "  FROM coingecko.gold.gold_resumo_moeda\r\n",
        "),\r\n",
        "\r\n",
        "total AS (\r\n",
        "  SELECT\r\n",
        "    SUM(valor_mercado_ultimo_dia_usd) AS total_mercado_usd\r\n",
        "  FROM base\r\n",
        ")\r\n",
        "\r\n",
        "SELECT\r\n",
        "  b.nome_moeda,\r\n",
        "  b.simbolo,\r\n",
        "  ROUND(b.valor_mercado_ultimo_dia_usd / t.total_mercado_usd * 100, 2)\r\n",
        "    AS participacao_percentual\r\n",
        "FROM base b\r\n",
        "CROSS JOIN total t\r\n",
        "ORDER BY participacao_percentual DESC\r\n",
        "LIMIT 10;\r\n"
      ]
    }
  ],
